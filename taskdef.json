{
  "family": "ecs-task-def",
  "networkMode": "awsvpc",
  "requiresCompatibilities": ["FARGATE"],
  "cpu": "256",
  "memory": "512",
  "executionRoleArn": "arn:aws:iam::329599647651:role/photo-uploader-task-execution-role",
  "taskRoleArn": "arn:aws:iam::329599647651:role/photo-uploader-task-role",
  "containerDefinitions": [
    {
      "name": "ecs-container",
      "image": "329599647651.dkr.ecr.eu-central-1.amazonaws.com/photo-uploader-repository:latest",
      "portMappings": [
        {
          "containerPort": 8080
        }
      ],
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "/ecs/cicdlab",
          "awslogs-region": "eu-central-1",
          "awslogs-stream-prefix": "cicdlab"
        }
      }
    }
  ]
}